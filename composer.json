{
    "name": "tjgazel/laravel-bs3-alert",
    "description": "Bootstrap 3 alerts for laravel 5.*",
    "keywords": ["laravel", "Boostrap 3", "Alerts", "Facade", "helper", "auto discover", "php"],
    "homepage": "https://github.com/tjgazel/laravel-bs4-alert",
    "license": "MIT",
    "type": "package",
    "authors": [
        {
            "name": "Talles Gazel",
            "email": "tjgazel@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "illuminate/support": ">=5.0.0",
        "illuminate/session": ">=5.0.0"
    },
    "autoload": {
        "files": [
            "helpers/helpers.php"
        ],
        "psr-4": {
            "TJGazel\\Bs3Alert\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "extra": {
        "laravel": {
            "providers": [
                "TJGazel\\Bs3Alert\\Bs3AlertServiceProvider"
            ],
            "aliases": {
                "Toastr": "TJGazel\\Bs3Alert\\Facades\\Bs3Alert"
            }
        }
    }
}
